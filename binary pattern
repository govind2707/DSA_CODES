#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	/*
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=(n-i+1);j++)
		cout<<j;
		cout<<endl;
	}*/
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=i;j++)
		{
			if((i+j)%2==0)
			cout<<1;
			else
			cout<<0;
		}
		cout<<endl;
	}
}
